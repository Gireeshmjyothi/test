public ReportingResponse<String> validateOtp(OtpValidationRequest otpValidationRequest){
        // Step 1 : Validate Otp validation request
        otpValidator.validateOtpValidationRequest(otpValidationRequest);
        OtpGenerationDto otpGenerationDto = otpManagementDao.updateOtpAndCaptchaStatus(otpValidationRequest.getRequestId(), RequestType.getRequestType(otpValidationRequest.getRequestType()));

        String responseMsg = MessageFormat.format(SUCCESS_MESSAGE, "OTP validated");
        //TODO-get token from token service
        return ReportingResponse.<String>build().status(RESPONSE_SUCCESS).data(List.of(responseMsg)).build();
    }
