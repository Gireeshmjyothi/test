CREATE TABLE FILE_CONFIG (
  ID                  UUID PRIMARY KEY,
  BANK_ID             INT,
  SFTP_PATH           VARCHAR(255),
  FILE_TYPE           VARCHAR(100),
  FILE_DELIMETER      VARCHAR(10),
  FILE_HEADER_ROW     INT,
  FILE_HEADER         VARCHAR(1000),
  FILE_HEADER_MAP     VARCHAR(2000),
  FILE_READ_FROM      INT,
  FILE_HASH_ROW       INT,
  FILE_HASH_HEADER    VARCHAR(500),
  FILE_SUMMARY_ROW    INT,
  FILE_SUMMARY_HEADER VARCHAR(500),
  FILE_ENCRYPTED      VARCHAR(50),
  FILE_ENC_LEVEL      VARCHAR(100),
  FILE_ENC_KEY        VARCHAR(255)
);
